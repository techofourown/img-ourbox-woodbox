# tools/versions.env
# Pinned versions for reproducible OurBox Woodbox builds.

# Ubuntu Server LTS ISO to base the installer on.
# NOTE: If you change this, re-test end-to-end.
UBUNTU_ISO_URL="https://releases.ubuntu.com/24.04.3/ubuntu-24.04.3-live-server-amd64.iso"

# k3s version for the airgapped platform
K3S_VERSION=v1.35.0+k3s1

# Images baked into the airgap payload (saved as tar files)
NGINX_IMAGE=docker.io/library/nginx:1.27-alpine
DUFS_IMAGE=docker.io/sigoden/dufs:v0.42.0
FLATNOTES_IMAGE=docker.io/dullage/flatnotes:v5.0.0
