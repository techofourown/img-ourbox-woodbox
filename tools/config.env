# tools/config.env
# Default identity inputs for this repo. Override via environment variables.

OURBOX_PRODUCT="ourbox"
OURBOX_DEVICE="woodbox"
OURBOX_TARGET="forge"
OURBOX_SKU="TOO-OBX-WBX-FORGE-JU3XK8"
